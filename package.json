{
  "name": "@krishnized/fonts",
  "version": "1.0.0",
  "description": "Google-ish fonts with enhanced unicode combining chars support for cyrillics.",
  "main": "index.js",
  "scripts": {
    "build-lora": "makeotf -ff ./Lora/diacritics.fea -f ./Lora/original/Lora-Italic.ttf -o ./Lora/Lora-Italic.otf",
    "build-lora-comb": "makeotf -ff ./Lora/diacritics.fea -gs -gf ./GlyphOrderAndAliasDB.combinations -r -f ./Lora/Lora-Italic.otf -o ./Lora/Lora-Italic-combinations.otf",
    "build-noto": "makeotf -ff ./Noto/diacritics.fea -r -f ./Noto/original/NotoSerif-Regular.otf -o ./Noto/NotoSerif-Regular.otf",
    "build-noto-italic": "makeotf -ff ./Noto/diacritics.fea -r -f ./Noto/original/NotoSerif-Italic.otf -o ./Noto/NotoSerif-Italic.otf",
    "build-noto-comb": "makeotf -ff ./Noto/diacritics.fea -gs -gf ./GlyphOrderAndAliasDB.combinations -r -f ./Noto/NotoSerif-Italic.otf -o ./Noto/NotoSerif-Italic-combinations.otf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krishnized/fonts.git"
  },
  "keywords": [
    "unicode",
    "lora",
    "noto",
    "google-fonts"
  ],
  "author": "dmitry iv",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krishnized/fonts/issues"
  },
  "homepage": "https://github.com/krishnized/fonts#readme"
}
