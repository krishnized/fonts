{
  "name": "@krishnized/fonts",
  "version": "1.0.0",
  "description": "Google-ish fonts with enhanced unicode combining chars support for cyrillics.",
  "main": "index.js",
  "scripts": {
    "build": "makeotf -ff ./font/diacritics.fea -f ./font/Lora-original.ttf -o ./font/Lora.otf",
    "build-lora": "makeotf -ff ./Lora/diacritics.fea -f ./Lora/original/Lora.ttf -o ./Lora/Lora.otf",
    "build-noto": "makeotf -ff ./Noto/diacritics.fea -f ./Noto/original/NotoSerif-Italic.otf -o ./Noto/NotoSerif-Italic.otf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krishnized/fonts.git"
  },
  "keywords": [
    "unicode",
    "lora",
    "noto",
    "google-fonts"
  ],
